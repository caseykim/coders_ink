<%= form_for @tattoo do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :url, "Image URL" %>
  <%= f.text_field :url %>
  <br>
  <%= f.submit "Submit", class: "button small success" %>
<% end %>
